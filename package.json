{
  "name": "react-bootstrap-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:serve": "./node_modules/.bin/serve -s build",
    "test": "react-scripts test",
    "test:coverage": "yarn test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js",
    "analyze": "source-map-explorer build/static/js/main.*",
    "precommit": "lint-staged",
    "size": "yarn build && size-limit",
    "cypress:run": "cypress run --record --key=${CYPRESS_RECORD_KEY}",
    "ci:cypress-run": "start-server-and-test start http://localhost:3000 cypress:run"
  },
  "dependencies": {
    "@loadable/component": "^5.10.3",
    "axios": "^0.19.0",
    "axios-hooks": "^1.7.1",
    "bootstrap": "^4.3.1",
    "react": "^16.11.0",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-dom": "^16.11.0",
    "react-infinite-scroller": "^1.2.4",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "styled-breakpoints": "^6.11.0",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@size-limit/preset-app": "^2.1.6",
    "@types/jest": "^24.0.22",
    "@types/loadable__component": "^5.10.0",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-bootstrap": "^0.32.20",
    "@types/react-dom": "^16.9.4",
    "@types/react-infinite-scroller": "^1.2.1",
    "@types/react-router-dom": "^5.1.2",
    "@types/styled-components": "^4.4.0",
    "cypress": "^3.6.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint-config-prettier": "^2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^3.0.9",
    "jest-styled-components": "^6.3.4",
    "lint-staged": "^9.4.2",
    "node-sass-chokidar": "^1.2.2",
    "prettier": "^1.19.1",
    "react-scripts": "^3.2.0",
    "serve": "^11.2.0",
    "source-map-explorer": "^2.1.0",
    "start-server-and-test": "^1.10.6",
    "typescript": "^3.7.2"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,scss}": [
      "./node_modules/.bin/eslint --fix",
      "git add"
    ]
  },
  "size-limit": [
    {
      "path": "build/static/**/*.{js,css,svg,woff,woff2,png,ttf,html}"
    }
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
